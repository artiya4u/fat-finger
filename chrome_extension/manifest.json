{
    "manifest_version": 3,
    "name": "Fat Finger",
    "description": "A Tinder bot based on user profile investment.",
    "version": "1.0.0",
    "icons": {
        "128": "icon.png"
    },
    "host_permissions": [
        "https://tinder.com/*"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://tinder.com/app/recs"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}
